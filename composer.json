{
    "name": "yaroslawww/laravel-cashier-subscription-pause",
    "description": "Support of Pausing and Resuming a Subscription.",
    "keywords": [
        "laravel",
        "cashier",
        "subscription",
        "pause"
    ],
    "homepage": "https://github.com/yaroslawww/laravel-cashier-subscription-pause",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yaroslav.georgitsa@gmail.com",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/cashier": "^10.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "orchestra/testbench": "^v5.20.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-laravel": "^1.4",
        "vimeo/psalm": "^4.10"
    },
    "autoload": {
        "psr-4": {
            "CashierSubscriptionPause\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CashierSubscriptionPause\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CashierSubscriptionPause\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
